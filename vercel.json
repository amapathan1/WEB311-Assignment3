{
  "version": 2,
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "memory": 3008,
      "runtime": "nodejs20.x"
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}